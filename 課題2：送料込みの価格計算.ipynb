   ],
   "source": [
    "price = input(\"税抜価格を入力してください：\")\n",
    "price_including_tax = round(float(price) * 1.1)\n",
    "if (price_including_tax >= 2000):\n",
    "    print(\"送料は無料です\")\n",
    "else:\n",
    "    print(\"送料として350円かかります\")\n",
    "    price_including_tax += 350\n",
    "\n",
    "print(\"送料込みの価格は\" + str(price_including_tax) + \"円です。\")"
   ]
  }
