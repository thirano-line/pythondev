    "def is_prime(value):\n",
    "    if (value == 1):\n",
    "        return False\n",
    "\n",
    "    nums = (2, 3, 5, 7)\n",
    "    for i in nums:\n",
    "        if (value % i == 0) and (value != i):\n",
    "            return False\n",
    "\n",
    "    return True\n",
    "\n",
    "for i in range(1, 100):\n",
    "    if (is_prime(i)):\n",
    "        print(i)"
   ]
