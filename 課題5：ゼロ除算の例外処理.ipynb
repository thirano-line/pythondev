    "except ValueError:\n",
    "    print(\"エラー: 数値を入力してください\")\n",
    "\n",
    "try:\n",
    "    c = a / b\n",
    "except ZeroDivisionError:\n",
    "    print(\"エラー: 0で割り算しないでください\")\n",
    "\n",
    "print(f\"{a} ÷  {b} = {c}\")\n",
    "\n",
    "print(\"処理を終了します\")"
   ]
